// Complexity: N*N

#include <bits/stdc++.h>
using namespace std;

int main(){
    int arr[] = { 3, 7, 2, 5, 8, 9 };
	int n = 6;

	for(int i = 0; i < n; i++){
        for(int j = i+1; j < n; j++){
            if(arr[i] > arr[j]){
                swap(arr[i], arr[j]);
            }
        }
	}
	for(int i = 0; i < n; i++){
        cout << arr[i] << " ";
	}
}
